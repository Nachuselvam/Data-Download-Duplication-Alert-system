{
    "manifest_version": 3,
    "name": "Download Duplicate Checker",
    "version": "1.0",
    "description": "Checks for duplicate files before downloading.",
    "permissions": ["downloads", "storage", "notifications", "http://localhost:5000/"],
    "background": {
      "service_worker": "script.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    }
  }
  